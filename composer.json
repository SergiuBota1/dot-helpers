{
  "name": "dotkernel/dot-helpers",
  "type": "library",
  "description": "Various helper structures and classes",
  "license": "MIT",
  "authors": [
    {
      "name": "DotKernel Team",
      "email": "team@dotkernel.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/http-message": "^1.0",
    "zendframework/zend-servicemanager": "^3.3.0",
    "zendframework/zend-expressive-helpers": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",
    "zendframework/zend-diactoros": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Helpers\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "1.1-dev"
    }
  }
}

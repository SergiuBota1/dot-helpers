{
    "name": "dotkernel/dot-helpers",
    "type": "library",
    "description": "Various helper structures and classes",
    "license": "MIT",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "psr/http-message": "^1.0",
        "laminas/laminas-servicemanager": "^3.3.0",
        "mezzio/mezzio-helpers": "^5.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "laminas/laminas-diactoros": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Helpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Helpers\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "3.0-dev"
        }
    }
}
